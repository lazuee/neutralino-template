{
	"applicationId": "lazuee.neutralino.template",
	"version": "0.0.0",
	"port": 3000,
	"defaultMode": "window",
	"documentRoot": "/www/",
	"url": "/",
	"enableServer": true,
	"enableNativeAPI": true,
	"exportAuthInfo": false,
	"nativeAllowList": ["app.*", "os.*", "storage.*", "window.*"],
	"modes": {
		"window": {
			"title": "Neutralino Template",
			"width": 800,
			"height": 500,
			"minWidth": 400,
			"minHeight": 200,
			"icon": "www/icons/app.png",
			"resizable": true,
			"enableInspector": false,
			"exitProcessOnClose": false
		},
		"browser": {},
		"cloud": {},
		"chrome": {
			"width": 800,
			"height": 500,
			"args": "--user-agent=\"Neutralinojs chrome mode\"",
			"nativeBlockList": ["filesystem.*", "os.*"]
		}
	},
	"cli": {
		"binaryName": "neutralino-template",
		"resourcesPath": "/www/",
		"extensionsPath": "/extensions/",
		"clientLibrary": "/www/js/neutralino.js",
		"binaryVersion": "4.7.0",
		"clientVersion": "3.6.0",
		"frontendLibrary": {
			"patchFile": "/www/index.html",
			"devUrl": "http://localhost:3000"
		}
	}
}
